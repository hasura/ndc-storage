                type: nullable
                underlying_type:
                  type: named
                  name: Boolean
            disableMultipart:
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: Boolean
            expires:
              type:
                type: nullable
                underlying_type:
                  type: named
                  name: TimestampTZ
            legalHold:
