              type:
                type: named
                name: Int32
            prefix:
              type:
                type: named
                name: String
            recursive:
              type:
                type: named
                name: Boolean
            startAfter:
              type:
                type: named
                name: String
            withMetadata:
              type:
                type: named
                name: Boolean
            withVersions:
              type:
                type: named
                name: Boolean
        NotificationCommonConfig:
